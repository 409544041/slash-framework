<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:inspectors="clr-namespace:BlueprintEditor.Inspectors">
    <Style TargetType="{x:Type inspectors:ListInspectorItem}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type inspectors:ListInspectorItem}">
                    <DockPanel>
                        <Button Name="PART_BtDelete" Width="25" Height="25" DockPanel.Dock="Left">-</Button>
                        <ContentPresenter ContentSource="Control" DockPanel.Dock="Right" />
                    </DockPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>