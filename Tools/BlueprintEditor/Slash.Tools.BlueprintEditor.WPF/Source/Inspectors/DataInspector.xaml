<inspectors:InspectorControl x:Class="BlueprintEditor.Inspectors.DataInspector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                                         xmlns:inspectors="clr-namespace:BlueprintEditor.Inspectors"
                                         mc:Ignorable="d" 
             d:DesignWidth = "400" d:DesignHeight="100" d:DataContext = "{d:DesignInstance inspectors:InspectorPropertyData}">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width = "200"></ColumnDefinition>
            <ColumnDefinition Width = "*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Label Name = "LbName" Grid.Column = "0">
            <Binding Path = "PropertyName"></Binding>
        </Label>
        <StackPanel Name = "Controls" Grid.Column = "1">
        </StackPanel>
    </Grid>
</inspectors:InspectorControl>
